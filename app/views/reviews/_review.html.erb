<div class="panel" id="<%= review.id %>">
<p>Header: <%= review.header  %></p>
<p>Rating: <%= review.rating  %></p>
<p>Review: <%= review.body  %></p>
<% if current_user %>
<div id="ballotBox">
  <a id="upvote" class="vote"> <%= image_tag("upvote.png") %> </a>
  <div id="review_score"> <%= review.score %> </div>
  <a id="downvote" class="vote"><%= image_tag("downvote.png") %></a>
<% end %>
</div>
<p>Created At: <%= review.created_at.to_date  %></p>
</div>
