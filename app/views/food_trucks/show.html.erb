<h1> <%=@food_truck.name %> </h1>
<ul>
  <h3>
    <%=@food_truck.avg_rating%>
  </h3>
  <p>
  Primary Location: <%=@food_truck.location%>
  </p>
  <p>
    <h5>
    <%=@food_truck.description%>
    </h5>
  </p>
</ul>

<h2>Reviews</h2>
<% @reviews.each do |review| %>
  <p><%= review.header  %></p>
  <p><%= review.rating  %></p>
  <p><%= review.body  %></p>
  <p><%= review.created_at  %></p>
<% end %>
